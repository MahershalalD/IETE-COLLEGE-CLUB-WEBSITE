<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>IETE-KEC | Class Performance</title>
    <link rel="stylesheet" href="styleevent.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
    <div class="loader-wrapper">
        <span class="loader"><span class="loader-inner"></span></span>
    </div>
    <div>
        <nav>
            <div class="nav-logo">
                <a href="/">
                    <img src="./images/iete.jpg" width="80px" height="60px">
                </a>

                <div class="burger">
                    <div class="line1"></div>
                    <div class="line2"></div>
                    <div class="line3"></div>
                </div>
            </div>
            <ul class="navbar-items">
                <li>
                    <a href="/points-table">Points-Table</a>
                </li>
                <li>
                    <a href="/classper">Class Performance</a>
                </li>
                <li>
                    <a href="/event">Events</a>
                </li>
                <li>
                    <a href="/results-user">Results</a>
                </li>
                <li>
                    <a href="/about">About</a>
                </li>
            </ul>
        </nav>
    </div>
 
<div class="grid-man"> 
    <div class="main_con">
        <% eventArray.forEach((item)=>{ %> 
        <div class="con_head">
            <h2><%=item.eventName%></h2>
        
        <div>
            <table class="tab">
               
                <tr>
                    <td>Event Type </td>
                    <td>:</td>
                    <td><%=item.eventType%></td>
                </tr>
                <tr>
                    <td>Event Date </td>
                    <td>:</td>
                    <td><%= item.eventDate %></td>
                </tr>
                <tr>
                    <td>Guest Name </td>
                    <td>:</td>
                    <td><%=item.eventGuest%></td>
                </tr>
                <tr>
                    <td>In Presence Of </td>
                    <td>:</td>
                    <td><%= item.eventPresence %></td>
                    <% item.eventComp.forEach((items,index)=>{ %>
                <tr>
                    <td>Event Competition <%= index+1 %> </td>
                    <td>:</td>
                    <td><%= items.name %>
                        <% if(items.link!==""){ %>
                        <a href="<%= items.link %>"><button class="btn-reg">Register</button></a>
                        <% } %>
                </tr>
                <% })%>
            </td>
            </table>
            </div>
        </div>
        <% })%>
        
    </div>
</div>
   
<script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc="
    crossorigin="anonymous"></script>
    <script src="animation.js"></script>
</body>

</html>